{
	"name": "andreasellwanger.com.website",
	"description": "Personal website of Andreas Ellwanger",
	"version": "1.0.0",
	"repository": "ndrsllwngr/andreasellwanger.com",
	"author": "Andreas Ellwanger",
	"homepage": "https://www.andreasellwanger.com",
	"bugs": {
		"url": "https://github.com/ndrsllwngr/andreasellwanger.com/issues"
	},
	"license": "UNLICENSED",
	"private": true,
	"engines": {
		"node": ">=12.16.0"
	},
	"scripts": {
		"dev": "next-remote-watch posts",
		"old-dev": "next dev",
		"start": "next start",
		"build": "yarn rss && next build",
		"old-build": "next build",
		"export": "next export",
		"export-preview": "cd out && python3 -m http.server",
		"prettify": "prettier --write ./components/**/*.js ./lib/**/*.js ./pages/**/*.js",
		"lint": "yarn prettify && eslint ./components/** ./lib/** ./pages/** --ext .js,.jsx",
		"lint:fix": "eslint ./components/** ./lib/** ./pages/**  --ext .js,.jsx --fix",
		"rss": "node lib/rss"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn lint"
		}
	},
	"dependencies": {
		"@fullhuman/postcss-purgecss": "2.3.0",
		"autoprefixer": "9.8.5",
		"classnames": "2.2.6",
		"framer-motion": "2.1.2",
		"lodash.debounce": "4.0.8",
		"next": "9.4.4",
		"nprogress": "0.2.0",
		"postcss-import": "12.0.1",
		"prop-types": "15.7.2",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"swr": "0.2.3",
		"tailwindcss": "1.5.2",
		"use-sound": "1.0.2"
	},
	"devDependencies": {
		"@svgr/webpack": "5.4.0",
		"@types/classnames": "2.2.10",
		"@types/node": "14.0.24",
		"@types/react": "16.9.43",
		"@typescript-eslint/eslint-plugin": "3.7.0",
		"@typescript-eslint/parser": "3.7.0",
		"babel-eslint": "10.1.0",
		"babel-plugin-module-resolver": "4.0.0",
		"eslint": "7.5.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-plugin-import": "2.22.0",
		"eslint-plugin-jsx-a11y": "6.3.1",
		"eslint-plugin-prettier": "3.1.4",
		"eslint-plugin-react": "7.20.3",
		"eslint-plugin-react-hooks": "4.0.8",
		"file-loader": "6.0.0",
		"gray-matter": "4.0.2",
		"husky": "4.2.5",
		"marked": "1.1.1",
		"next-remote-watch": "0.2.0",
		"prettier": "2.0.5",
		"prism-react-renderer": "1.1.1",
		"rss": "1.2.2",
		"typescript": "3.9.7"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"prettier": {
		"printWidth": 120,
		"semi": false,
		"singleQuote": true
	}
}
